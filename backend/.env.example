# ICS Cybersecurity Platform Environment Variables
# Copy this file to .env and update with your actual values

# Security - CRITICAL: Change these in production!
SECRET_KEY=your-secret-key-here-minimum-32-characters
JWT_SECRET=your-jwt-secret-here-minimum-32-characters

# Database
DATABASE_URL=postgresql://ics_user:ics_password@localhost:5432/ics_cybersecurity

# Redis
REDIS_URL=redis://localhost:6379/0
REDIS_PASSWORD=

# External Services
THREAT_INTEL_API_KEY=

# Email Notifications
SMTP_SERVER=
SMTP_USERNAME=
SMTP_PASSWORD=

# Webhook Notifications
WEBHOOK_URL=
WEBHOOK_SECRET=

# Application Settings
DEBUG=false
LOG_LEVEL=INFO